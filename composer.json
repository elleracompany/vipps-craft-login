{
  "name": "vippsas/vipps-craft-login",
  "description": "Vipps Login Integration for CraftCMS",
  "version": "2.0.0",
  "type": "craft-plugin",
  "keywords": ["vipps", "login", "authentication", "craft", "craftcms"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ellera AS",
      "homepage": "https://ellera.no"
    }
  ],
  "support": {
    "email": "support@ellera.no",
    "issues": "https://github.com/elleracompany/vipps-craft-login/issues",
    "source": "https://github.com/elleracompany/vipps-craft-login",
    "docs": "https://github.com/elleracompany/vipps-craft-login"
  },
  "require": {
    "php": "^8.0",
    "craftcms/cms": "^4.0",
    "guzzlehttp/guzzle": "^7.2.0",
    "ext-json": "*"
